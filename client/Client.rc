IDC_CLIENT MENU DISCARDABLE 
BEGIN
    POPUP "&Main"
    BEGIN
        MENUITEM "&Go to table",                IDC_GOTO_GAME
        MENUITEM "&Waiting list...",            IDC_JOIN_WAIT_LIST
        MENUITEM SEPARATOR
        MENUITEM "(no open table windows)",     IDM_GOTO_TABLE1, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Cashier...",                  IDC_CASHIER
        MENUITEM SEPARATOR
        MENUITEM "Log in...",                   IDC_LOG_IN
        MENUITEM SEPARATOR
        MENUITEM "Log off and E&xit",           IDC_EXIT
    END
    POPUP "&Games"
    BEGIN
        MENUITEM "&Hold 'em",                   IDM_SHOW_HOLDEM, CHECKED
        MENUITEM "&Omaha Hi",                   IDM_SHOW_OMAHA
        MENUITEM "Omaha &Hi/Lo 8 or better",    IDM_SHOW_OMAHA_HI_LO
        MENUITEM "&7 Card Stud",                IDM_SHOW_7CS
        MENUITEM "7 Card Stud Hi/Lo 8 or better", IDM_SHOW_7CS_HI_LO
        MENUITEM "One on One",                  IDM_SHOW_1ON1
        MENUITEM "Hide &play money tables",     IDC_HIDE_PLAY_MONEY
    END
    POPUP "&Options"
    BEGIN
        MENUITEM "&All-Ins remaining...",       IDM_OPTIONS_ALLINS
        MENUITEM "&Request All-Ins reset...",   IDM_REQUEST_ALLIN_RESET
        MENUITEM "Request &Hand History...",    IDC_REQUEST_HAND_HISTORY
        MENUITEM SEPARATOR
        MENUITEM "Change/Validate &Email address...", IDM_CHANGE_EMAIL
        MENUITEM "Edit Player &Information",    IDM_CHANGE_ADDRESS
        MENUITEM "Change &Password...",         IDM_CHANGE_PASSWORD
        MENUITEM SEPARATOR
        MENUITEM "Enable A&nimation",           IDM_ENABLE_ANIMATION
        , CHECKED
        MENUITEM "Enable &Sounds",              IDM_ENABLE_SOUNDS, CHECKED
        MENUITEM "Use &4 Color Deck",           IDM_ENABLE_4COLOR_DECK
        MENUITEM "Table Themes (Skins)",        IDM_TABLE_THEMES, GRAYED
    END
    POPUP "&Player Services"
    BEGIN
        MENUITEM "&Promotions",                 IDC_PROMOTIONUPDATES
        MENUITEM "&Help/Support",               IDC_ONLINE_HELP
        MENUITEM SEPARATOR
        MENUITEM "&About EmediaSoftware.com Client...", IDM_ABOUT
    END
END


